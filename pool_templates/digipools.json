        {
        "coin": "CLO",
        "servers": [
            {
                "geo": null,
                "urls": [
                    "clo.digipools.org:3502"
                ]
            }
        ],
        "miners": {
            "claymore": {
                "epools_tpl": "POOL: %URL%, WALLET: %WAL%, WORKER: %WORKER_NAME%, PSW: x",
                "claymore_user_config": "-allcoins %COIN% -allpools 0 -esm 0 -showdiff 1"
            },
            "ethminer": {
                "cuda": 1,
                "opencl": 1,
                "pass": "x",
                "port": "%URL_PORT%",
                "server": "stratum1+tcp://%URL_HOST%",
                "template": "%WAL%"
            },
            "asicminer": {
                "url": "stratum+tcp://%URL%",
                "template": "%WAL%"
            },
            "phoenixminer": {
                "url": "POOL: %URL%, WALLET: %WAL%, WORKER: %WORKER_NAME%",
                "user_config": "-coin %COIN%"
            },
            "finminer": {
                "algo": "ethash",
                "url": "%URL%",
                "template": "%WAL%",
                "pass": "x",
                "user_config": "coin = %COIN%\nrigName = %WORKER_NAME%"
            },
            "bminer": {
                "url": "%URL%",
                "algo": "ethash",
                "template": "%WAL%",
                "pass": "x" 
            },
            "nanominer": {
                "algo": "ethash",
                "url": "%URL%",
                "template": "%WAL%",
                "pass": "x",
                "user_config": "coin = %COIN%\nrigName = %WORKER_NAME%"
            },
            "nbminer": {
                "url": "%URL%",
                "algo": "ethash",
                "pass": "x",
                "template": "%WAL%"
            },
            "teamredminer": {
                "algo": "ethash",
                "url": "%URL%",
                "template": "%WAL%",
                "pass": "x",
                "worker":"%WORKER_NAME%"
            },
            "lolminer": {
                "algo": "ETHASH",
                "template": "%WAL%",
                "server": "%URL_HOST%",
                "port": "%URL_PORT%",
                "pass": "x",
                "worker":"%WORKER_NAME%",
                "user_config": "--ethstratum=ETHV1"
            }
        }
