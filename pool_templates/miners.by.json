			},
			"ethminer": {
				"cuda": 1,
				"opencl": 0,
				"pass": "x",
				"port": "%URL_PORT%",
				"server": "stratum1+tcp://%URL_HOST%",
                "template": "%WAL%.%WORKER_NAME%"
			},
            "asicminer": {
                "url": "stratum+tcp://%URL%",
                "template": "%WAL%.%WORKER_NAME%"
            },
            "phoenixminer": {
                "url": "POOL: %URL%, WAL: %WAL%, PASS: x, WORKER: %WORKER_NAME%, PROTO: 2, COIN: %COIN%"
            },
            "finminer": {
                "algo": "ethash",
                "url": "%URL%",
                "template": "%WAL%",
                "pass": "x",
                "user_config": "coin = %COIN%\nrigName = %WORKER_NAME%"
            },
            "bminer": {
                "algo": "ethash",
