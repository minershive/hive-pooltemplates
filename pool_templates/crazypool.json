[
	{ // pool header section - not necessary
        "pool": {
            "name": "CrazyPool",        	// pool name
            "url": "https://crazypool.org",	// pool URL
            "fee": 1,                   	// pool fee in persent
            "type": "PPLNS / SOLO"			// pool reward system
        }
    },
	{
        "coin": "B2G",
        "servers": [
            {
                "geo": "US-PPLNS",
                "urls": [
                    "b2g.crazypool.org:7650"
                ]
            },
            {
                "geo": "US-SOLO",
                "urls": [
                    "solo-b2g.crazypool.org:7450"
                ]
            }
        ],
        "miners": {
            "claymore": {
                "epools_tpl": "POOL: %URL%, WALLET: %WAL%, WORKER: %WORKER_NAME%, PSW: x",
                "claymore_user_config": "-allcoins 1 -allpools 0 -esm 0"
            },
            "ethminer": {
                "cuda": 1,
                "opencl": 0,
                "pass": "x",
                "port": "%URL_PORT%",
                "server": "stratum1+tcp://%URL_HOST%",
                "template": "%WAL%.%WORKER_NAME%"
            },
            "asicminer": {
                "url": "stratum+tcp://%URL%",
